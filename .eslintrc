{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "comma-dangle": 0,
    "new-cap": 0,
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "react/jsx-no-bind": [1, {
    "ignoreRefs": false,
    "allowArrowFunctions": true,
    "allowBind": true
    }],
    "max-len": 0,
    no-param-reassign: [2, { "props": false }]
  }
}
